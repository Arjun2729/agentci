{
  "name": "agentci",
  "version": "0.1.0",
  "description": "CI guardrails / regression tests for agent side effects",
  "license": "MIT",
  "type": "commonjs",
  "bin": {
    "agentci": "dist/cli/main.js"
  },
  "files": [
    "dist",
    "policy-packs",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint 'src/**/*.ts' 'tests/**/*.ts'",
    "lint:fix": "eslint 'src/**/*.ts' 'tests/**/*.ts' --fix",
    "format": "prettier --write 'src/**/*.ts' 'tests/**/*.ts'",
    "format:check": "prettier --check 'src/**/*.ts' 'tests/**/*.ts'",
    "typecheck": "tsc --noEmit",
    "check": "npm run lint && npm run format:check && npm run typecheck && npm run test",
    "prepublishOnly": "npm run check && npm run build",
    "demo": "bash examples/run_demo.sh",
    "screencast": "bash examples/run_screencast.sh"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "^12.1.0",
    "picomatch": "^4.0.2",
    "psl": "^1.9.0",
    "yaml": "^2.4.5",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/node": "^20.14.10",
    "@types/picomatch": "^4.0.2",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "@vitest/coverage-v8": "^2.1.9",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.8",
    "fast-check": "^3.23.2",
    "prettier": "^3.8.1",
    "tsup": "^8.1.0",
    "tsx": "^4.16.2",
    "typescript": "^5.5.4",
    "vitest": "^2.0.5"
  },
  "engines": {
    "node": ">=18.17"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Arjun2729/agentci.git"
  },
  "keywords": [
    "ai",
    "agent",
    "observability",
    "flight-recorder",
    "policy",
    "governance",
    "tracing",
    "security"
  ]
}
